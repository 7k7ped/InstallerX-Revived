    <string name="allow_test">Allow Test</string>
    <string name="allow_test_desc">If the package being installed is a test-only app, allow it to be installed.</string>
    <string name="internal">Internal</string>
    <string name="internal_desc">Force the package to be installed to internal storage.</string>
    <string name="external">External</string>
    <string name="external_desc">Force the package to be installed to external storage.</string>
    <string name="from_adb">From ADB</string>
    <string name="from_adb_desc">Tells the system that the install request was made by ADB.</string>
    <string name="all_users">All Users</string>
    <string name="all_users_desc">Install the package for all users on the system, instead of just the specified or default user.</string>
    <string name="allow_downgrade">Allow Downgrade</string>
    <string name="allow_downgrade_desc">Attempts to downgrade the currently-installed version of the package to the one being installed.</string>
    <string name="grant_all_permissions">Grant All Requested Permissions</string>
    <string name="grant_all_permissions_desc">Automatically grant all permissions the package is requesting. Note that this only applies to runtime permissions. Special permissions like All Files Access or Accessibility won\'t be automatically granted.</string>
    <string name="force_volume_uuid">Force Volume UUID</string>
    <string name="force_volume_uuid_desc">Allows specifying a volume UUID to install the package to.</string>
    <string name="force_permission_prompt">Force Permission Prompt</string>
    <string name="force_permission_prompt_desc">Forces even system installers to show the install confirmation prompt.</string>
    <string name="instant_app">Instant App</string>
    <string name="instant_app_desc">Install the package as an instant app.</string>
    <string name="dont_kill_app">Don\'t Kill App</string>
    <string name="dont_kill_app_desc">Used when installing a feature for a split app to prevent the main app from being killed during install.</string>
    <string name="full_app">Full App</string>
    <string name="full_app_desc">Indicates that the package being installed is a full app. Can be used to upgrade instant apps to full apps.</string>
    <string name="allocate_aggressive">Allocate Aggressive</string>
    <string name="allocate_aggressive_desc">Indicates that the package is \"critical to system health or security\" and will more aggressively delete clearable files to make space if needed.</string>
    <string name="virtual_preload">Virtual Preload</string>
    <string name="virtual_preload_desc">Indicates that the package is a virtual preload.</string>
    <string name="apex">APEX</string>
    <string name="apex_desc">Indicates that the package being installed is an APEX package.</string>
    <string name="enable_rollback">Enable Rollback</string>
    <string name="enable_rollback_desc">Allow future rollbacks of this package.</string>
    <string name="disable_verification">Disable Verification</string>
    <string name="disable_verification_desc">Disables package verification during install. This is not signature verification.</string>
    <string name="staged">Staged</string>
    <string name="staged_desc">Indicates this package is being installed as part of a staged install.</string>
    <string name="all_whitelist_restricted_permissions">Allow Restricted Permissions</string>
    <string name="all_whitelist_restricted_permissions_desc">Allow restricted permissions to be granted for the package being installed.</string>
    <string name="disable_allowed_apex_update_check">Disable Allowed APEX Update Check</string>
    <string name="disable_allowed_apex_update_check_desc">Don\'t check whether the installed APEX can be updated.</string>
    <string name="bypass_low_target_sdk_block">Bypass Low Target SDK Block</string>
    <string name="bypass_low_target_sdk_block_desc">Allows apps targeting old SDK versions to be installed.</string>
    <string name="dry_run">Dry Run</string>
    <string name="dry_run_desc">Verify the package, but don\'t install it.</string>
    <string name="replace_existing">Replace Existing</string>
    <string name="replace_existing_desc">If the package being installed already exists, replace/upgrade it.</string>
